       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:local="clr-namespace:H.Maui.App.Main"
       Shell.FlyoutBehavior="Flyout">
    <Shell.Resources>
        <Style TargetType="ShellContent">
            <Setter Property="Title" Value="{Binding Source={RelativeSource AncestorType={x:Type BaseShellItem}}, Path=Title}" />
            <Setter Property="FlyoutIcon" Value="{StaticResource IconTwo}" />
            <Setter Property="Icon" Value="{StaticResource IconTwo}" />
        </Style>
        <Style TargetType="FlyoutItem">
            <Setter Property="FlyoutIcon" Value="{StaticResource IconOne}" />
        </Style>
        <Style TargetType="Tab">
            <Setter Property="Icon" Value="{StaticResource IconThree}" />
        </Style>
    </Shell.Resources>

    <FlyoutItem Title="Button">
        <ShellContent Title="Button" Route="OnePage">
